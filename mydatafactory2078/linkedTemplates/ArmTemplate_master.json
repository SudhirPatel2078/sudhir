{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mydatafactory2078"
		},
		"linkedAzureSQLDatabse_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedAzureSQLDatabse'"
		},
		"AzureDataLakeStorageEMP_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageEMP'"
		},
		"AzureSqlDatabaseEMP_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseEMP'"
		},
		"AzureDataLakeStorageEMP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mystorageacc2078.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "mydatafactory2078_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"linkedAzureSQLDatabse_connectionString": {
						"value": "[parameters('linkedAzureSQLDatabse_connectionString')]"
					},
					"AzureDataLakeStorageEMP_accountKey": {
						"value": "[parameters('AzureDataLakeStorageEMP_accountKey')]"
					},
					"AzureSqlDatabaseEMP_connectionString": {
						"value": "[parameters('AzureSqlDatabaseEMP_connectionString')]"
					},
					"AzureDataLakeStorageEMP_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageEMP_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}